<?xml version="1.0" encoding="utf-8" ?>
<!-- A simple piece of xml to control the export behaviours of the ODBC exporter prog -->

<configuration> 

     <company name="CMS" connectionstring="dsn=SHG CMS" >
         <Export tableview="dagent" exportname="dagent" required="true" Sqlcommand=
         "SELECT  * , TODAY run_date from root.dagent where row_date = today - 1" />

         <Export tableview="dsplit" exportname="dsplit" required="true" Sqlcommand=
         "SELECT  * , TODAY run_date from root.dsplit where row_date = today - 1" />

         <Export tableview="dvdn" exportname="dvdn" required="true" Sqlcommand=
         "SELECT  * , TODAY run_date from root.dvdn where row_date = today - 1" />

         <Export tableview="haglog" exportname="haglog" required="true" Sqlcommand=
         "SELECT  * , TODAY run_date from root.haglog where row_date = today - 1" />

         <Export tableview="hsplit" exportname="hsplit" required="true" Sqlcommand=
         "SELECT  * , TODAY run_date from root.hsplit where row_date = today - 1" />

   </company>    
  
  
</configuration>